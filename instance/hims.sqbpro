-- Drop existing tables to apply structural changes (if needed)
DROP TABLE IF EXISTS &quot;disease_risk_factors&quot;;
DROP TABLE IF EXISTS &quot;disease_labs&quot;;
DROP TABLE IF EXISTS &quot;disease_keywords&quot;;
DROP TABLE IF EXISTS &quot;disease_management_plans&quot;;
DROP TABLE IF EXISTS &quot;disease_symptoms&quot;;
DROP TABLE IF EXISTS &quot;symptoms&quot;;
DROP TABLE IF EXISTS &quot;diseases&quot;;

-- Create updated diseases table with prevalence and last_updated
CREATE TABLE IF NOT EXISTS &quot;diseases&quot; (
    &quot;id&quot; INTEGER,
    &quot;name&quot; TEXT NOT NULL UNIQUE,
    &quot;cui&quot; TEXT NOT NULL,
    &quot;description&quot; TEXT,
    &quot;prevalence&quot; TEXT, -- New column for prevalence data
    &quot;last_updated&quot; TIMESTAMP DEFAULT CURRENT_TIMESTAMP, -- New column for tracking updates
    PRIMARY KEY(&quot;id&quot; AUTOINCREMENT)
);

-- Create updated symptoms table with last_updated
CREATE TABLE IF NOT EXISTS &quot;symptoms&quot; (
    &quot;id&quot; INTEGER,
    &quot;name&quot; TEXT NOT NULL UNIQUE,
    &quot;cui&quot; TEXT NOT NULL,
    &quot;description&quot; TEXT,
    &quot;last_updated&quot; TIMESTAMP DEFAULT CURRENT_TIMESTAMP, -- New column for tracking updates
    PRIMARY KEY(&quot;id&quot; AUTOINCREMENT)
);

-- Create disease_symptoms table with updated foreign key constraints
CREATE TABLE IF NOT EXISTS &quot;disease_symptoms&quot; (
    &quot;disease_id&quot; INTEGER,
    &quot;symptom_id&quot; INTEGER,
    FOREIGN KEY(&quot;symptom_id&quot;) REFERENCES &quot;symptoms&quot;(&quot;id&quot;) ON DELETE CASCADE ON UPDATE CASCADE,
    FOREIGN KEY(&quot;disease_id&quot;) REFERENCES &quot;diseases&quot;(&quot;id&quot;) ON DELETE CASCADE ON UPDATE CASCADE,
    PRIMARY KEY(&quot;disease_id&quot;,&quot;symptom_id&quot;)
);

-- Create updated disease_management_plans with severity and plan_type
CREATE TABLE IF NOT EXISTS &quot;disease_management_plans&quot; (
    &quot;id&quot; INTEGER,
    &quot;disease_id&quot; INTEGER NOT NULL,
    &quot;plan&quot; TEXT NOT NULL,
    &quot;severity&quot; TEXT CHECK(&quot;severity&quot; IN ('mild', 'moderate', 'severe')), -- New column for severity
    &quot;plan_type&quot; TEXT CHECK(&quot;plan_type&quot; IN ('acute', 'chronic', 'preventive')), -- New column for plan type
    &quot;created_at&quot; TIMESTAMP DEFAULT CURRENT_TIMESTAMP,
    FOREIGN KEY(&quot;disease_id&quot;) REFERENCES &quot;diseases&quot;(&quot;id&quot;) ON DELETE CASCADE ON UPDATE CASCADE,
    PRIMARY KEY(&quot;id&quot; AUTOINCREMENT)
);

-- Create disease_keywords table
CREATE TABLE IF NOT EXISTS &quot;disease_keywords&quot; (
    &quot;id&quot; INTEGER,
    &quot;disease_id&quot; INTEGER NOT NULL,
    &quot;keyword&quot; TEXT NOT NULL,
    &quot;cui&quot; TEXT,
    FOREIGN KEY(&quot;disease_id&quot;) REFERENCES &quot;diseases&quot;(&quot;id&quot;) ON DELETE CASCADE ON UPDATE CASCADE,
    PRIMARY KEY(&quot;id&quot; AUTOINCREMENT)
);

-- Create disease_labs table
CREATE TABLE IF NOT EXISTS &quot;disease_labs&quot; (
    &quot;id&quot; INTEGER,
    &quot;disease_id&quot; INTEGER NOT NULL,
    &quot;lab_test&quot; TEXT NOT NULL,
    &quot;description&quot; TEXT,
    FOREIGN KEY(&quot;disease_id&quot;) REFERENCES &quot;diseases&quot;(&quot;id&quot;) ON DELETE CASCADE ON UPDATE CASCADE,
    PRIMARY KEY(&quot;id&quot; AUTOINCREMENT)
);

-- Create new disease_risk_factors table
CREATE TABLE IF NOT EXISTS &quot;disease_risk_factors&quot; (
    &quot;id&quot; INTEGER,
    &quot;disease_id&quot; INTEGER NOT NULL,
    &quot;risk_factor&quot; TEXT NOT NULL,
    &quot;description&quot; TEXT,
    FOREIGN KEY(&quot;disease_id&quot;) REFERENCES &quot;diseases&quot;(&quot;id&quot;) ON DELETE CASCADE ON UPDATE CASCADE,
    PRIMARY KEY(&quot;id&quot; AUTOINCREMENT)
);

