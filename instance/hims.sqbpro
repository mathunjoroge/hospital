INSERT INTO &quot;disease_management_plans&quot; VALUES (1,1,'Diagnosis:\n   - Confirm via clinical signs (fever, cough, dyspnea), CXR, or CT chest.\n   - Use CURB-65 score for severity; procalcitonin for bacterial etiology.\n\nTreatment:\n   - Outpatient: Amoxicillin-clavulanate or doxycycline.\n   - Inpatient: Ceftriaxone + azithromycin; consider levofloxacin for MDR.\n   - Viral: Oseltamivir or baloxavir for influenza.\n\nMonitoring:\n   - SpO2, CRP, serial CXR for non-responders.\n\nComplications:\n   - ARDS, empyema, sepsis.\n\nSupportive Care:\n   - Oxygen therapy, IV fluids, antipyretics.\n\nReferral:\n   - ICU for SpO2 &lt;92% or septic shock.\n\nPrevention:\n   - PCV15/PCV20 vaccination, smoking cessation, flu vaccine.','moderate','acute','2025-09-05 14:10:00');
INSERT INTO &quot;disease_management_plans&quot; VALUES (2,2,'Diagnosis:\n   - ECG (ST elevation, Q waves), troponin I/T, coronary angiogram.\n\nTreatment:\n   - Aspirin 300 mg, ticagrelor, heparin; PCI within 90 min.\n   - SGLT2 inhibitors (e.g., dapagliflozin) for post-MI heart failure.\n   - Beta-blockers, high-dose statins, ACEi.\n\nMonitoring:\n   - Cardiac enzymes, echo for LVEF, Holter for arrhythmias.\n\nComplications:\n   - Cardiogenic shock, ventricular rupture, heart failure.\n\nSupportive Care:\n   - Pain control (morphine), oxygen if SpO2 &lt;90%.\n\nReferral:\n   - Immediate cath lab; cardiology follow-up.\n\nPrevention:\n   - Smoking cessation, DASH diet, cardiac rehab.','severe','acute','2025-09-05 14:10:00');
INSERT INTO &quot;disease_management_plans&quot; VALUES (3,3,'Diagnosis:\n   - Non-contrast CT, MRI (DWI), NIHSS score.\n\nTreatment:\n   - Alteplase within 4.5 hrs; tenecteplase for large vessel occlusion.\n   - Thrombectomy within 24 hrs if LVO.\n   - BP control &lt;180/100 mmHg post-thrombolysis.\n\nMonitoring:\n   - Neuro checks q1h, repeat imaging at 24 hrs.\n\nComplications:\n   - Intracranial hemorrhage, cerebral edema.\n\nSupportive Care:\n   - Stroke unit, swallow assessment, DVT prophylaxis.\n\nReferral:\n   - Stroke team, endovascular intervention.\n\nPrevention:\n   - Dual antiplatelets, statins, BP control.','severe','acute','2025-09-05 14:10:00');
INSERT INTO &quot;disease_management_plans&quot; VALUES (4,4,'Diagnosis:\n   - Clinical history, stool PCR for pathogens.\n\nTreatment:\n   - Oral rehydration solution (ORS); zinc in children &lt;5.\n   - Antibiotics (azithromycin) for shigella, severe cases only.\n   - Ondansetron for vomiting.\n\nMonitoring:\n   - Hydration status, stool output, electrolytes.\n\nComplications:\n   - Severe dehydration, HUS in E. coli infection.\n\nSupportive Care:\n   - Probiotics, bland diet post-recovery.\n\nReferral:\n   - Persistent symptoms, dysentery.\n\nPrevention:\n   - Safe water, handwashing, food safety.','mild','acute','2025-09-05 14:10:00');
INSERT INTO &quot;disease_management_plans&quot; VALUES (5,5,'Diagnosis:\n   - Urinalysis, urine culture, point-of-care PCR for resistance.\n\nTreatment:\n   - Nitrofurantoin 100 mg BID x 5 days; fosfomycin 3 g single dose.\n   - Avoid TMP-SMX in high-resistance areas.\n\nMonitoring:\n   - Symptom resolution within 48 hrs, repeat culture if recurrent.\n\nComplications:\n   - Pyelonephritis, urosepsis.\n\nSupportive Care:\n   - Phenazopyridine for dysuria, hydration.\n\nReferral:\n   - Complicated UTI, pregnancy, or males.\n\nPrevention:\n   - Cranberry supplements, post-coital voiding.','mild','acute','2025-09-05 14:10:00');
INSERT INTO &quot;disease_management_plans&quot; VALUES (6,6,'Diagnosis:\n   - Spirometry (FEV1/FVC &lt;0.7 post-bronchodilator), FeNO test.\n\nTreatment:\n   - GINA guidelines: SABA PRN for mild; ICS-formoterol for maintenance.\n   - Dupilumab or omalizumab for severe allergic asthma.\n\nMonitoring:\n   - Peak flow, ACT score, exacerbation history.\n\nComplications:\n   - Status asthmaticus, respiratory failure.\n\nSupportive Care:\n   - Inhaler technique training, allergen avoidance.\n\nReferral:\n   - Pulmonology for biologics or frequent exacerbations.\n\nPrevention:\n   - Flu vaccine, smoking cessation, action plan.','moderate','chronic','2025-09-05 14:10:00');
INSERT INTO &quot;disease_management_plans&quot; VALUES (7,7,'Diagnosis:\n   - HbA1c ≥6.5%, fasting glucose ≥126 mg/dL, CGM data.\n\nTreatment:\n   - Metformin + SGLT2 inhibitors or GLP-1 agonists for T2DM.\n   - Insulin pump/CGM for T1DM or advanced T2DM.\n\nMonitoring:\n   - HbA1c q3 months, lipid profile, microalbuminuria.\n\nComplications:\n   - DKA, HHS, macrovascular disease.\n\nSupportive Care:\n   - Carb counting, exercise, psychological support.\n\nReferral:\n   - Endocrinology for complex cases, CGM initiation.\n\nPrevention:\n   - Prediabetes screening, weight loss programs.','moderate','chronic','2025-09-05 14:10:00');
INSERT INTO &quot;disease_management_plans&quot; VALUES (8,8,'Diagnosis:\n   - BP ≥130/80 mmHg (3 readings), ABPM for white-coat HTN.\n\nTreatment:\n   - Lifestyle: DASH diet, 150 min/week exercise.\n   - Thiazide-like diuretics, ARBs, or CCBs; spironolactone for resistant HTN.\n\nMonitoring:\n   - Home BP monitoring, electrolytes, eGFR.\n\nComplications:\n   - LVH, stroke, renal failure.\n\nSupportive Care:\n   - Salt restriction, stress management.\n\nReferral:\n   - Resistant HTN, secondary causes.\n\nPrevention:\n   - Regular BP screening, weight control.','moderate','chronic','2025-09-05 14:10:00');
INSERT INTO &quot;disease_management_plans&quot; VALUES (9,9,'Diagnosis:\n   - History, physical exam; MRI/CT for red flags (e.g., cauda equina).\n\nTreatment:\n   - NSAIDs (ibuprofen), PT, cognitive behavioral therapy.\n   - Duloxetine for chronic pain; epidural steroids for radiculopathy.\n\nMonitoring:\n   - Pain VAS score, Oswestry Disability Index.\n\nComplications:\n   - Chronic pain syndrome, neurological deficits.\n\nSupportive Care:\n   - Core exercises, ergonomic adjustments.\n\nReferral:\n   - Spine specialist for persistent symptoms, surgery.\n\nPrevention:\n   - Posture training, lifting techniques.','mild','chronic','2025-09-05 14:10:00');
INSERT INTO &quot;disease_management_plans&quot; VALUES (10,10,'Diagnosis:\n   - RDT, microscopy, or PCR for Plasmodium species.\n\nTreatment:\n   - ACT (artemether-lumefantrine) for uncomplicated; IV artesunate for severe.\n   - Primaquine for P. vivax/ovale (G6PD testing required).\n\nMonitoring:\n   - Parasite clearance, hemoglobin, renal function.\n\nComplications:\n   - Cerebral malaria, severe anemia, ARDS.\n\nSupportive Care:\n   - Hydration, antipyretics, glucose monitoring.\n\nReferral:\n   - Severe malaria, organ dysfunction.\n\nPrevention:\n   - LLINs, IRS, malaria vaccine (R21/Matrix-M).','severe','acute','2025-09-05 14:10:00');
INSERT INTO &quot;disease_management_plans&quot; VALUES (11,11,'Diagnosis:\n   - Serology (HBsAg, anti-HCV), HBV/HCV PCR, FibroScan.\n\nTreatment:\n   - HBV: Tenofovir alafenamide or entecavir.\n   - HCV: Glecaprevir/pibrentasvir for 8–12 weeks.\n   - HAV: Supportive care.\n\nMonitoring:\n   - Viral load, LFTs, HCC screening (ultrasound).\n\nComplications:\n   - Cirrhosis, HCC, portal HTN.\n\nSupportive Care:\n   - Alcohol cessation, low-fat diet.\n\nReferral:\n   - Hepatology for advanced fibrosis, transplant.\n\nPrevention:\n   - HBV/HAV vaccines, safe needle practices.','moderate','chronic','2025-09-05 14:10:00');
INSERT INTO &quot;disease_management_plans&quot; VALUES (12,12,'Diagnosis:\n   - NS1 antigen, RT-PCR, IgM/IgG serology.\n\nTreatment:\n   - Supportive: Paracetamol, IV fluids for severe cases.\n   - Avoid NSAIDs to prevent bleeding risk.\n\nMonitoring:\n   - Hematocrit, platelets, warning signs (e.g., mucosal bleeding).\n\nComplications:\n   - Dengue hemorrhagic fever, shock syndrome.\n\nSupportive Care:\n   - Oral hydration, rest, mosquito bite prevention.\n\nReferral:\n   - ICU for warning signs, severe bleeding.\n\nPrevention:\n   - Dengue vaccine (CYD-TDV), vector control.','severe','acute','2025-09-05 14:10:00');
INSERT INTO &quot;disease_management_plans&quot; VALUES (13,13,'Diagnosis:\n   - Blood/stool culture, PCR for Salmonella typhi.\n\nTreatment:\n   - Ceftriaxone or azithromycin for 7–14 days.\n   - Fluoroquinolones avoided due to resistance.\n\nMonitoring:\n   - Fever resolution, CBC, LFTs.\n\nComplications:\n   - Relapse, carrier state, perforation.\n\nSupportive Care:\n   - ORS, high-calorie diet post-recovery.\n\nReferral:\n   - Surgical consult for perforation.\n\nPrevention:\n   - ViCPS or Ty21a vaccine, sanitation.','moderate','acute','2025-09-05 14:10:00');
INSERT INTO &quot;disease_management_plans&quot; VALUES (14,14,'Diagnosis:\n   - Xpert MTB/RIF Ultra, sputum culture, chest CT.\n\nTreatment:\n   - RIPE (rifampicin, isoniazid, pyrazinamide, ethambutol) for 6 months.\n   - Bedaquiline for MDR-TB.\n\nMonitoring:\n   - Sputum AFB q2 months, LFTs, visual acuity.\n\nComplications:\n   - MDR/XDR-TB, hemoptysis, pneumothorax.\n\nSupportive Care:\n   - Nutritional support, B6 supplementation.\n\nReferral:\n   - ID specialist for MDR-TB, surgical resection.\n\nPrevention:\n   - Isoniazid prophylaxis, ventilation control.','severe','chronic','2025-09-05 14:10:00');
INSERT INTO &quot;disease_management_plans&quot; VALUES (15,15,'Diagnosis:\n   - qSOFA/SOFA score, blood cultures, lactate, PCT.\n\nTreatment:\n   - Broad-spectrum antibiotics (e.g., meropenem + vancomycin) within 1 hr.\n   - Fluid resuscitation (30 mL/kg), norepinephrine for shock.\n\nMonitoring:\n   - Lactate clearance, MAP &gt;65 mmHg, urine output.\n\nComplications:\n   - MODS, DIC, ARDS.\n\nSupportive Care:\n   - Mechanical ventilation, glycemic control.\n\nReferral:\n   - ICU, ID consult for source control.\n\nPrevention:\n   - Early infection treatment, vaccination.','severe','acute','2025-09-05 14:10:00');
INSERT INTO &quot;disease_management_plans&quot; VALUES (16,16,'Diagnosis:\n   - CBC, peripheral smear, ferritin, TIBC, B12/folate.\n\nTreatment:\n   - Ferrous sulfate for IDA; IV iron for intolerance.\n   - B12 injections for pernicious anemia; transfusions for severe cases.\n\nMonitoring:\n   - Hb, reticulocyte count, MCV.\n\nComplications:\n   - High-output heart failure, neuro deficits.\n\nSupportive Care:\n   - Dietary iron sources, vitamin C.\n\nReferral:\n   - Hematology for refractory cases, bone marrow biopsy.\n\nPrevention:\n   - Iron-fortified foods, prenatal screening.','mild','chronic','2025-09-05 14:10:00');
INSERT INTO &quot;disease_management_plans&quot; VALUES (17,17,'Diagnosis:\n   - PHQ-9, GAD-7, clinical interview, suicide risk.\n\nTreatment:\n   - CBT, SSRIs (sertraline, escitalopram), or ketamine for TRD.\n   - TMS or ECT for severe refractory cases.\n\nMonitoring:\n   - PHQ-9 score, suicidal ideation, sleep patterns.\n\nComplications:\n   - Self-harm, social withdrawal, substance use.\n\nSupportive Care:\n   - Peer support groups, mindfulness training.\n\nReferral:\n   - Psychiatry for suicidality, inpatient care.\n\nPrevention:\n   - Mental health screening, resilience training.','moderate','chronic','2025-09-05 14:10:00');
INSERT INTO &quot;disease_management_plans&quot; VALUES (18,18,'Diagnosis:\n   - GAD-7, SCID-5, rule out medical mimics.\n\nTreatment:\n   - CBT, SSRIs/SNRIs (e.g., venlafaxine), buspirone for GAD.\n   - Benzodiazepines short-term for acute panic.\n\nMonitoring:\n   - Anxiety severity, medication side effects.\n\nComplications:\n   - Phobias, substance dependence.\n\nSupportive Care:\n   - Relaxation techniques, sleep hygiene.\n\nReferral:\n   - Psychiatry for complex cases, therapy resistance.\n\nPrevention:\n   - Stress management, early CBT.','moderate','chronic','2025-09-05 14:10:00');
INSERT INTO &quot;disease_management_plans&quot; VALUES (19,19,'Diagnosis:\n   - Spirometry (FEV1/FVC &lt;0.7), DLCO, high-res CT.\n\nTreatment:\n   - LABA/LAMA (e.g., umeclidinium/vilanterol), ICS for exacerbations.\n   - Roflumilast for severe COPD with chronic bronchitis.\n\nMonitoring:\n   - CAT score, exacerbation frequency, SpO2.\n\nComplications:\n   - Cor pulmonale, pneumothorax, infections.\n\nSupportive Care:\n   - Pulmonary rehab, oxygen therapy.\n\nReferral:\n   - Pulmonology for LTOT or surgical options.\n\nPrevention:\n   - Smoking cessation, annual flu/pneumo vaccines.','severe','chronic','2025-09-05 14:10:00');
INSERT INTO &quot;disease_management_plans&quot; VALUES (20,20,'Diagnosis:\n   - NT-proBNP, echo (LVEF), chest X-ray.\n\nTreatment:\n   - ARNI (sacubitril/valsartan), beta-blockers, SGLT2i.\n   - Loop diuretics for congestion, ICD for low EF.\n\nMonitoring:\n   - Daily weights, eGFR, potassium levels.\n\nComplications:\n   - Cardiogenic shock, sudden cardiac death.\n\nSupportive Care:\n   - Fluid/salt restriction, cardiac rehab.\n\nReferral:\n   - Cardiology for device therapy, transplant.\n\nPrevention:\n   - BP control, lipid management.','severe','chronic','2025-09-05 14:10:00');
INSERT INTO &quot;disease_management_plans&quot; VALUES (21,21,'Diagnosis:\n   - EEG, MRI brain, seizure semiology.\n\nTreatment:\n   - Levetiracetam or lamotrigine first-line; perampanel for refractory cases.\n   - VNS or ketogenic diet for intractable epilepsy.\n\nMonitoring:\n   - Seizure log, AED levels, EEG.\n\nComplications:\n   - Status epilepticus, SUDEP.\n\nSupportive Care:\n   - Seizure safety, sleep hygiene.\n\nReferral:\n   - Neurology, epilepsy surgery evaluation.\n\nPrevention:\n   - Medication adherence, trigger avoidance.','moderate','chronic','2025-09-05 14:10:00');
INSERT INTO &quot;disease_management_plans&quot; VALUES (22,22,'Diagnosis:\n   - eGFR &lt;60 mL/min, urine ACR, renal ultrasound.\n\nTreatment:\n   - ACEi/ARBs, SGLT2i (e.g., empagliflozin), finerenone for proteinuria.\n   - Dialysis or transplant for ESRD.\n\nMonitoring:\n   - eGFR, electrolytes, BP control.\n\nComplications:\n   - ESRD, hyperkalemia, cardiovascular events.\n\nSupportive Care:\n   - Low-protein diet, smoking cessation.\n\nReferral:\n   - Nephrology for eGFR &lt;30, dialysis planning.\n\nPrevention:\n   - DM/HTN control, regular screening.','severe','chronic','2025-09-05 14:10:00');
INSERT INTO &quot;disease_management_plans&quot; VALUES (23,23,'Diagnosis:\n   - X-ray (joint space narrowing), clinical exam.\n\nTreatment:\n   - Acetaminophen, topical NSAIDs, duloxetine.\n   - Intra-articular hyaluronic acid or PRP for knees.\n\nMonitoring:\n   - Pain scores, joint function, WOMAC index.\n\nComplications:\n   - Joint deformity, reduced mobility.\n\nSupportive Care:\n   - Weight loss, PT, assistive devices.\n\nReferral:\n   - Orthopedics for joint replacement.\n\nPrevention:\n   - Exercise, weight management.','moderate','chronic','2025-09-05 14:10:00');
INSERT INTO &quot;disease_management_plans&quot; VALUES (24,24,'Diagnosis:\n   - RF, anti-CCP, ESR/CRP, DAS28 score.\n\nTreatment:\n   - Methotrexate + hydroxychloroquine; JAKi (e.g., baricitinib) for moderate-severe.\n   - Anti-TNF (e.g., adalimumab) for DMARD failure.\n\nMonitoring:\n   - DAS28, joint counts, LFTs.\n\nComplications:\n   - Joint destruction, infections, CVD.\n\nSupportive Care:\n   - PT, smoking cessation, vaccinations.\n\nReferral:\n   - Rheumatology for early DMARDs, biologics.\n\nPrevention:\n   - Early treatment, infection monitoring.','moderate','chronic','2025-09-05 14:10:00');
INSERT INTO &quot;disease_management_plans&quot; VALUES (25,25,'Diagnosis:\n   - ICHD-3 criteria, headache diary, rule out secondary causes.\n\nTreatment:\n   - Acute: Rimegepant or ubrogepant; triptans if tolerated.\n   - Preventive: CGRP inhibitors (e.g., erenumab), topiramate.\n\nMonitoring:\n   - MIDAS score, attack frequency.\n\nComplications:\n   - Chronic migraine, medication overuse headache.\n\nSupportive Care:\n   - Trigger avoidance, stress management.\n\nReferral:\n   - Neurology for chronic or refractory cases.\n\nPrevention:\n   - Lifestyle changes, prophylactic therapy.','moderate','chronic','2025-09-05 14:10:00');
INSERT INTO &quot;disease_management_plans&quot; VALUES (26,26,'Diagnosis:\n   - TSH &gt;4 mIU/L, low free T4, thyroid ultrasound.\n\nTreatment:\n   - Levothyroxine (1.6 mcg/kg/day), titrate q6 weeks.\n   - Liothyronine for resistant cases (investigational).\n\nMonitoring:\n   - TSH q6–12 months, free T4, cardiac symptoms.\n\nComplications:\n   - Myxedema coma, CVD risk.\n\nSupportive Care:\n   - Warm clothing, dietary iodine.\n\nReferral:\n   - Endocrinology for myxedema, pregnancy.\n\nPrevention:\n   - Iodine supplementation, screening in elderly.','mild','chronic','2025-09-05 14:10:00');
INSERT INTO &quot;disease_management_plans&quot; VALUES (27,27,'Diagnosis:\n   - TSH &lt;0.1 mIU/L, elevated T3/T4, TRAb.\n\nTreatment:\n   - Methimazole (5–30 mg/day), propranolol for tachycardia.\n   - Radioiodine or surgery for definitive treatment.\n\nMonitoring:\n   - Thyroid function q4–6 weeks, CBC, LFTs.\n\nComplications:\n   - Thyroid storm, atrial fibrillation.\n\nSupportive Care:\n   - Smoking cessation, eye lubrication.\n\nReferral:\n   - Endocrinology, ophthalmology for TED.\n\nPrevention:\n   - Regular monitoring, stress reduction.','moderate','chronic','2025-09-05 14:10:00');
INSERT INTO &quot;disease_management_plans&quot; VALUES (28,28,'Diagnosis:\n   - 4th-gen HIV test, viral load PCR, resistance testing.\n\nTreatment:\n   - ART: Biktarvy (bictegravir/emtricitabine/TAF) first-line.\n   - Cabotegravir/rilpivirine (long-acting injectable) for adherent patients.\n\nMonitoring:\n   - Viral load q3–6 months, CD4, renal/liver function.\n\nComplications:\n   - Opportunistic infections, IRIS, lipodystrophy.\n\nSupportive Care:\n   - Adherence counseling, vaccinations.\n\nReferral:\n   - ID specialist for all cases, resistance.\n\nPrevention:\n   - PrEP, condoms, needle exchange.','severe','chronic','2025-09-05 14:10:00');
INSERT INTO &quot;disease_management_plans&quot; VALUES (29,29,'Diagnosis:\n   - Rapid antigen test, RT-PCR, clinical symptoms.\n\nTreatment:\n   - Baloxavir or oseltamivir within 48 hrs for high-risk.\n   - Symptomatic care for low-risk patients.\n\nMonitoring:\n   - Respiratory status, secondary infections.\n\nComplications:\n   - Pneumonia, myocarditis, encephalitis.\n\nSupportive Care:\n   - Hydration, antipyretics, rest.\n\nReferral:\n   - Pulmonology for severe respiratory distress.\n\nPrevention:\n   - Annual flu vaccine, hand hygiene.','mild','acute','2025-09-05 14:10:00');
INSERT INTO &quot;disease_management_plans&quot; VALUES (30,30,'Diagnosis:\n   - Clinical diagnosis, PCR for atypical cases.\n\nTreatment:\n   - Symptomatic: Decongestants, saline nasal spray.\n   - Antihistamines for allergic component.\n\nMonitoring:\n   - Symptom duration (&gt;10 days suggests bacterial).\n\nComplications:\n   - Sinusitis, otitis media.\n\nSupportive Care:\n   - Hydration, steam inhalation.\n\nReferral:\n   - ENT for persistent symptoms, complications.\n\nPrevention:\n   - Handwashing, avoid sick contacts.','mild','acute','2025-09-05 14:10:00');
INSERT INTO &quot;disease_management_plans&quot; VALUES (31,31,'Diagnosis:\n   - ANA, anti-dsDNA, anti-Smith, complement levels, SLICC criteria.\n\nTreatment:\n   - Hydroxychloroquine for all; prednisone for flares.\n   - Voclosporin or belimumab for lupus nephritis.\n   - Rituximab for severe organ involvement.\n\nMonitoring:\n   - SLEDAI, complement, proteinuria, ESR/CRP.\n\nComplications:\n   - Lupus nephritis, CNS lupus, thrombosis.\n\nSupportive Care:\n   - UV protection, vaccinations, smoking cessation.\n\nReferral:\n   - Rheumatology, nephrology for organ damage.\n\nPrevention:\n   - Early immunosuppression, flare prevention.','moderate','chronic','2025-09-05 14:10:00');
INSERT INTO &quot;disease_management_plans&quot; VALUES (32,32,'Diagnosis:\n   - Anti-GAD, anti-islet antibodies, HbA1c, C-peptide.\n\nTreatment:\n   - Insulin (basal-bolus or pump), CGM required.\n   - Teplizumab for stage 2 T1DM (pre-symptomatic).\n\nMonitoring:\n   - Glucose, HbA1c q3 months, DKA risk.\n\nComplications:\n   - Hypoglycemia, retinopathy, nephropathy.\n\nSupportive Care:\n   - Diabetes education, mental health support.\n\nReferral:\n   - Endocrinology for CGM, islet transplant.\n\nPrevention:\n   - Autoantibody screening in family members.','severe','chronic','2025-09-05 14:10:00');
INSERT INTO &quot;disease_management_plans&quot; VALUES (33,33,'Diagnosis:\n   - Clinical (plaque psoriasis), PASI, skin biopsy.\n\nTreatment:\n   - Topical corticosteroids, calcipotriene for mild.\n   - IL-17/IL-23 inhibitors (e.g., bimekizumab) for moderate-severe.\n   - Apremilast or deucravacitinib for resistant cases.\n\nMonitoring:\n   - PASI, DLQI, joint symptoms, LFTs.\n\nComplications:\n   - Psoriatic arthritis, metabolic syndrome.\n\nSupportive Care:\n   - Emollients, UV protection, weight loss.\n\nReferral:\n   - Dermatology, rheumatology for PsA.\n\nPrevention:\n   - Trigger avoidance, stress reduction.','moderate','chronic','2025-09-05 14:10:00');
INSERT INTO &quot;disease_management_plans&quot; VALUES (34,34,'Diagnosis:\n   - Anti-SSA/SSB, salivary gland biopsy, Schirmer test.\n\nTreatment:\n   - Artificial tears/saliva, pilocarpine for sicca.\n   - Rituximab or belimumab for systemic disease.\n\nMonitoring:\n   - Sicca symptoms, lymphoma risk, RF/ANA.\n\nComplications:\n   - Lymphoma, parotid enlargement.\n\nSupportive Care:\n   - Oral hygiene, eye protection, hydration.\n\nReferral:\n   - Rheumatology, ophthalmology for complications.\n\nPrevention:\n   - Regular lymphoma screening, early treatment.','moderate','chronic','2025-09-05 14:10:00');
INSERT INTO &quot;disease_management_plans&quot; VALUES (35,35,'Diagnosis:\n   - TSH, free T4/T3, TRAb, thyroid scintigraphy.\n\nTreatment:\n   - Methimazole or propylthiouracil; beta-blockers.\n   - Teprotumumab for severe TED.\n   - Radioiodine or thyroidectomy for relapse.\n\nMonitoring:\n   - TFTs q4 weeks, LFTs, eye exam.\n\nComplications:\n   - Thyroid storm, ophthalmopathy, bone loss.\n\nSupportive Care:\n   - Smoking cessation, selenium supplementation.\n\nReferral:\n   - Endocri for TED, surgical consult.\n\nPrevention:\n   - Iodine control, stress management.','moderate','chronic','2025-09-05 14:10:00');
INSERT INTO &quot;disease_management_plans&quot; VALUES (36,36,'Diagnosis:\n   - AChR antibodies, EMG, single-fiber EMG.\n\nTreatment:\n   - Pyridostigmine, prednisone, azathioprine.\n   - Rozanolixizumab or eculizumab for refractory MG.\n   - Thymectomy for AChR-positive patients.\n\nMonitoring:\n   - MG-ADL score, respiratory function, QMG.\n\nComplications:\n   - Myasthenic crisis, thymoma.\n\nSupportive Care:\n   - PT, avoid aminoglycosides, vaccinations.\n\nReferral:\n   - Neurology, thoracic surgery for thymectomy.\n\nPrevention:\n   - Infection control, medication adherence.','moderate','chronic','2025-09-05 14:10:00');
INSERT INTO &quot;disease_management_plans&quot; VALUES (37,37,'Diagnosis:\n   - Anti-smooth muscle, ANA, IgG, liver biopsy.\n\nTreatment:\n   - Prednisone (40–60 mg/day), azathioprine maintenance.\n   - Mycophenolate for steroid intolerance.\n\nMonitoring:\n   - LFTs, IgG, fibrosis staging (FibroScan).\n\nComplications:\n   - Cirrhosis, HCC, liver failure.\n\nSupportive Care:\n   - Alcohol avoidance, low-fat diet.\n\nReferral:\n   - Hepatology for transplant, advanced disease.\n\nPrevention:\n   - Early immunosuppression, HCC screening.','moderate','chronic','2025-09-05 14:10:00');
INSERT INTO &quot;disease_management_plans&quot; VALUES (38,38,'Diagnosis:\n   - Anti-centromere/Scl-70, skin biopsy, mRSS.\n\nTreatment:\n   - Mycophenolate for skin/ILD, nifedipine for Raynaud’s.\n   - Riociguat for PAH, rituximab for refractory ILD.\n\nMonitoring:\n   - mRSS, PFTs, echo for PAH.\n\nComplications:\n   - Scleroderma renal crisis, ILD, GI dysmotility.\n\nSupportive Care:\n   - Skin moisturizers, PT, cold avoidance.\n\nReferral:\n   - Rheumatology, pulmonology for ILD/PAH.\n\nPrevention:\n   - Early immunosuppression, Raynaud’s protection.','severe','chronic','2025-09-05 14:10:00');
INSERT INTO &quot;disease_management_plans&quot; VALUES (39,39,'Diagnosis:\n   - Recurrent ulcers, pathergy test, HLA-B51.\n\nTreatment:\n   - Colchicine for mucocutaneous; adalimumab for severe.\n   - Apremilast for oral ulcers, azathioprine for systemic.\n\nMonitoring:\n   - Ulcer frequency, ESR/CRP, eye exams.\n\nComplications:\n   - Uveitis, DVT, CNS involvement.\n\nSupportive Care:\n   - Pain relief, patient education.\n\nReferral:\n   - Rheumatology, ophthalmology for uveitis.\n\nPrevention:\n   - Flare management, infection control.','moderate','chronic','2025-09-05 14:10:00');
INSERT INTO &quot;disease_management_plans&quot; VALUES (40,40,'Diagnosis:\n   - Proteinuria (&gt;500 mg/day), renal biopsy, anti-dsDNA.\n\nTreatment:\n   - Mycophenolate or cyclophosphamide for induction.\n   - Voclosporin + hydroxychloroquine for maintenance.\n\nMonitoring:\n   - UPCR, eGFR, complement levels.\n\nComplications:\n   - ESRD, hypertension, infections.\n\nSupportive Care:\n   - Low-salt diet, BP control, vaccinations.\n\nReferral:\n   - Nephrology, rheumatology for management.\n\nPrevention:\n   - Early SLE control, renal screening.','severe','chronic','2025-09-05 14:10:00');
INSERT INTO &quot;disease_management_plans&quot; VALUES (41,41,'Diagnosis:\n   - McDonald criteria, MRI (T2/FLAIR lesions), CSF bands.\n\nTreatment:\n   - High-efficacy DMTs: ocrelizumab, cladribine for RRMS.\n   - Siponimod or ponesimod for SPMS.\n   - Fenebrutinib (BTK inhibitor, investigational).\n\nMonitoring:\n   - MRI q6–12 months, EDSS, JC virus serology.\n\nComplications:\n   - PML, disability progression, spasticity.\n\nSupportive Care:\n   - PT, vitamin D, cognitive rehab.\n\nReferral:\n   - Neurology for DMTs, neuro-rehab.\n\nPrevention:\n   - Early DMT, infection monitoring.','moderate','chronic','2025-09-05 14:10:00');
INSERT INTO &quot;disease_management_plans&quot; VALUES (42,42,'Diagnosis:\n   - Colonoscopy, fecal calprotectin, CT/MR enterography.\n\nTreatment:\n   - Budesonide for mild; ustekinumab or risankizumab for moderate-severe.\n   - Surgery for fistulas, strictures.\n\nMonitoring:\n   - CDAI, CRP, calprotectin, colonoscopy q2–3 yrs.\n\nComplications:\n   - Abscesses, colorectal cancer, malnutrition.\n\nSupportive Care:\n   - Enteral nutrition, smoking cessation.\n\nReferral:\n   - IBD specialist, surgery for complications.\n\nPrevention:\n   - Maintenance therapy, CRC screening.','moderate','chronic','2025-09-05 14:10:00');
INSERT INTO &quot;disease_management_plans&quot; VALUES (43,43,'Diagnosis:\n   - Colonoscopy, Mayo score, fecal calprotectin.\n\nTreatment:\n   - Mesalamine for mild-moderate; tofacitinib or ozanimod for severe.\n   - Colectomy for refractory or toxic megacolon.\n\nMonitoring:\n   - Mayo score, CRP, colonoscopy q1–2 yrs.\n\nComplications:\n   - Toxic megacolon, colorectal cancer.\n\nSupportive Care:\n   - Low-fiber diet during flares, hydration.\n\nReferral:\n   - IBD specialist, colorectal surgery.\n\nPrevention:\n   - Adherence to 5-ASA, CRC surveillance.','moderate','chronic','2025-09-05 14:10:00');
INSERT INTO &quot;disease_management_plans&quot; VALUES (44,44,'Diagnosis:\n   - CK elevation, anti-Jo-1, muscle biopsy, MRI muscle.\n\nTreatment:\n   - Prednisone (1 mg/kg/day), methotrexate or tacrolimus.\n   - IVIG or rituximab for refractory cases.\n\nMonitoring:\n   - CK, muscle strength, PFTs for ILD.\n\nComplications:\n   - ILD, malignancy, dysphagia.\n\nSupportive Care:\n   - PT, sun protection, aspiration precautions.\n\nReferral:\n   - Rheumatology, pulmonology for ILD.\n\nPrevention:\n   - Early immunosuppression, cancer screening.','moderate','chronic','2025-09-05 14:10:00');
INSERT INTO &quot;disease_management_plans&quot; VALUES (45,45,'Diagnosis:\n   - Clinical history (post-COVID symptoms &gt;3 months), rule out mimics.\n\nTreatment:\n   - Symptomatic: Pacing for fatigue, beta-blockers for POTS.\n   - Multidisciplinary rehab for cognitive and respiratory issues.\n\nMonitoring:\n   - Symptom tracking, 6-minute walk test, cognitive assessment.\n\nComplications:\n   - Dysautonomia, ME/CFS, pulmonary fibrosis.\n\nSupportive Care:\n   - Energy conservation, mental health support.\n\nReferral:\n   - Long COVID clinic, pulmonology, cardiology.\n\nPrevention:\n   - COVID-19 vaccination, early antiviral use.','moderate','chronic','2025-09-05 14:10:00');
INSERT INTO &quot;disease_management_plans&quot; VALUES (46,46,'Diagnosis:\n   - Liver ultrasound, FibroScan, ALT/AST elevation.\n\nTreatment:\n   - Lifestyle: Weight loss (7–10%), Mediterranean diet.\n   - Pioglitazone or vitamin E for NASH; obeticholic acid (investigational).\n\nMonitoring:\n   - LFTs, FibroScan, lipid profile, HbA1c.\n\nComplications:\n   - Cirrhosis, HCC, cardiovascular disease.\n\nSupportive Care:\n   - Exercise, alcohol avoidance, DM control.\n\nReferral:\n   - Hepatology for NASH, advanced fibrosis.\n\nPrevention:\n   - Obesity prevention, regular liver screening.','moderate','chronic','2025-09-05 14:10:00');

